{
  "name": "swellyo",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web && cp public/swelly_chat.html dist/ && node update-bundle.js && node create-amplify-files.js",
    "build:netlify": "npm run build:netlify:pre && expo export --platform web && npm run build:netlify:post",
    "build:netlify:pre": "([ -f next.config.js ] && mv next.config.js next.config.js.bak || true) && ([ -d pages ] && mv pages pages.bak || true)",
    "build:netlify:post": "node update-bundle.js && node create-netlify-redirects.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "5.0.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.80.0",
    "@types/node": "^24.3.0",
    "expo": "^54.0.22",
    "expo-auth-session": "~7.0.8",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-video": "^3.0.14",
    "expo-web-browser": "~15.0.9",
    "next": "^15.5.2",
    "posthog-react-native": "^4.19.0",
    "react": "19.1.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^3.15.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/helper-define-polyfill-provider": "^0.6.5",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
